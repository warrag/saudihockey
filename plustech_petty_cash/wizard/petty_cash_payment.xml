<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="wizard_financial_view_form" model="ir.ui.view">
            <field name="name">account.transfer.wizard</field>
            <field name="model">account.transfer.wizard</field>
            <field name="arch" type="xml">
                <form string="wizard_financial_form">
                    <sheet>
                        <group>
                            <group>
                                <field name="res_id" invisible="1"/>
                                <field name="payment_journal_id"/>
                                <field name="petty_analytic_account_id"/>
                                <field name="petty_transfer_account_id"/>
                            </group>
                        </group>
                    </sheet>
                    <footer>
                        <button name="action_wizard_approve" type="object" class="oe_highlight" string="Approve"/>
                    </footer>
                </form>
            </field>
        </record>



    </data>
</odoo>
