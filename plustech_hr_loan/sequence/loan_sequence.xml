<?xml version="1.0" ?>
<odoo>
    <data noupdate='1'>
        <record id="hr_cash_loan_sequence" model="ir.sequence">
            <field name="name">Cash Loan Request</field>
            <field name="code">hr.loan.seq</field>
            <field name="prefix">LO/</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>
        <record id="hr_other_loan_sequence" model="ir.sequence">
            <field name="name">Bank Loan Request</field>
            <field name="code">other.loan.seq</field>
            <field name="prefix">BLO/</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>

        <!-- Sequences for Skipp installment -->
        <record id="dev_skip_installment" model="ir.sequence">
            <field name="name">Skip installment Sequence</field>
            <field name="code">hr.skip.installment</field>
            <field name="prefix">SKP-INS/</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>

    </data>
</odoo>